@model Cliente
@{
    Layout = "_Layout";
}

<div class="container">
    <div class="col-sm-10">
      <h5 class="text-danger">Perfil</h5>
    </div>
    
  <form asp-asp-asp-controller="cliente" asp-action="update" method="POST">
   <div class="form-group">
      <label class="control-label col-sm-2" for="nome">Nome:</label>
            <div class="col-sm-10">
                <input type="nome" class="form-control" id="nome" placeholder="Nome" name="Nome" asp-for="Nome">
                <span asp-validation-for="Nome"></span>
            </div>
    </div>

    <div class="form-group">
      <label class="control-label col-sm-2" for="cpf">Cpf:</label>
            <div class="col-sm-10">
                <input type="nome" class="form-control" id="cpf" placeholder="Cpf" name="Cpf" asp-for="Cpf">
                <span asp-validation-for="Cpf"></span>
            </div>
    </div>

    <div class="form-group">
      <label class="control-label col-sm-2" for="celular">Celular:</label>
            <div class="col-sm-10">
                <input type="nome" class="form-control" id="celular" placeholder="Celular" name="Celular" asp-for="Celular">
                <span asp-validation-for="Celular"></span>
            </div>
    </div>
  
    <div class="form-group">        
      <div class="col-sm-offset-2 col-sm-10">
        <button class="btn btn-danger">Salvar</button>
        <a href="/" class="btn btn-default">Voltar</a>
    </div>
    </div>
  </form>
</div>

    
    
    
    